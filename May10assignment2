student_details = {1:["Anusha",'BCA',980],
                2:["sailaja",'MPCS',964],
                3:["rishikesh",'MECS',683],
                4:["jairam",'BBA',466]} 
print ("The dictionary before performing remove is:", 
(student_details))
del student_details[4] 
print ("The dictionary after remove is :",
(student_details))
